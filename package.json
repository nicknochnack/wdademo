{
  "name": "watson-discovery-ui",
  "version": "1.0.0",
  "private": true,
  "main": "app.js",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/IBM/watson-discovery-ui.git"
  },
  "scripts": {
    "start": "node app.js",
    "start:watch": "nodemon app.js",
    "bootstrap": "cp env.sample .env",
    "build": "browserify -r ./public/js/bundle.js --extension=js -t [babelify --presets [ react env ] envify] -t [uglifyify --global=true] -o ./public/js/bundle.min.js && browserify -r ./public/js/home.js --extension=js -t [babelify --presets [ react env ] envify] -t [uglifyify --global=true] -o ./public/js/home.min.js",
    "test -u": "jest -u",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.3",
    "cfenv": "^1.1.0",
    "chart.js": "^2.7.3",
    "dotenv": "^6.1.0",
    "envify": "^4.1.0",
    "express": "^4.17.3",
    "express-react-views": "^0.10.5",
    "ip-range-check": "0.0.2",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "minifyify": "^7.3.5",
    "moment": "^2.19.4",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.1",
    "react": "^16.0.0",
    "react-chartjs-2": "^2.6.4",
    "react-dom": "^16.0.0",
    "react-scrollbar": "^0.5.1",
    "react-tagcloud": "^1.3.1",
    "semantic-ui-react": "^0.77.2",
    "sort-by": "^1.2.0",
    "uglifyify": "^4.0.5",
    "vcap_services": "^0.4.0",
    "watson-developer-cloud": "^3.13.0",
    "watson-speech": "^0.35.4"
  },
  "engines": {
    "node": "8.9.4"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "ignore": [
      "node_modules"
    ]
  },
  "devDependencies": {
    "dezalgo": "~1.0",
    "enzyme": "^3.7.0",
    "eslint": "^4.13.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^3.0.1",
    "nodemon": "^1.18.6",
    "react-test-renderer": "^15.6.2"
  }
}
